<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>egov</groupId>
    <artifactId>sra</artifactId>
    <version>1.0.1</version>
    <packaging>war</packaging>
    <name>sra</name>
    <organization>
        <name>전자정부</name>
    </organization>
    <description>공공기관 프로젝트</description>
    <url>http://www.egovframe.go.kr</url>

    <licenses>
        <license>
            <name>The Apache Software License, Version 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
        </license>
    </licenses>

    <properties>
        <java.version>1.8</java.version>
        <jdk.version>1.8</jdk.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <egovframework.rte.version>3.10.0</egovframework.rte.version>
        <spring.version>4.3.25.RELEASE</spring.version>
        <com.fasterxml.jackson.version>2.12.5</com.fasterxml.jackson.version>
    </properties>

    <repositories>
        <repository>
            <id>egovframe</id>
            <url>http://maven.egovframe.kr:8080/maven/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>in-project</id>
            <name>outlaw-jars</name>
            <url>file://${project.basedir}/lib</url>
        </repository>
        <repository>
            <id>in-jenkins</id>
            <name>outlaw-jars</name>
            <url>file://${user.dir}/lib</url>
        </repository>
    </repositories>

    <dependencies>
        <dependency>
            <groupId>egovframework.rte</groupId>
